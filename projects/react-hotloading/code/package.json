{
  "name": "liveprogramming_react-hotloading",
  "author": "Markus Petrykowski",
  "version": "0.1.0",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "node --harmony server",
    "start-production": "NODE_ENV=production PORT=8000 node server > webapp.log 2> webapp-err.log &",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "test": "npm run test-unit",
    "lint": "eslint Nexboard/src/js Nexboard/test Portal/src/js Portal/test --ext js,jsx",
    "fix-lint": "eslint --fix Nexboard/src/js Nexboard/test Portal/src/js Portal/test --ext js,jsx",
    "test-unit": "rimraf coverage && nyc _mocha test/unit"
  },
  "dependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "css-loader": "^0.19.0",
    "es6-promise": "^3.2.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "font-awesome-webpack": "0.0.4",
    "html-webpack-plugin": "^1.6.1",
    "http": "0.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.3",
    "less": "^2.7.1",
    "mysql": "^2.11.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-thunk": "^2.1.0",
    "require-all": "^2.0.0",
    "rimraf": "^2.4.3",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "canvas": "^1.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^4.18.2",
    "eslint-plugin-mocha": "^3.0.0",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.1",
    "flux-standard-action": "^0.6.1",
    "ghooks": "^1.0.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "nyc": "^6.4.4",
    "redux-devtools-dispatch": "^2.2.0",
    "redux-mock-store": "^1.1.0",
    "selenium-server-standalone": "^2.53.1",
    "timekeeper": "^0.1.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "exclude": [
      "test/**/*.js",
      "test/**/*.jsx",
      "node_modules",
      "test",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
